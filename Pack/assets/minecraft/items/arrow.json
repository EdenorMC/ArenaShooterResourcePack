{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "next",
	"model": {
          "type": "minecraft:model",
	  "model": "minecraft:item/next"
	}
      },
      {
        "when": "previous",
	"model": {
          "type": "minecraft:model",
	  "model": "minecraft:item/previous"
	}
      },
      {
        "when": "back",
	"model": {
          "type": "minecraft:model",
	  "model": "minecraft:item/back"
	}
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/redstone"
    }
  }
}
